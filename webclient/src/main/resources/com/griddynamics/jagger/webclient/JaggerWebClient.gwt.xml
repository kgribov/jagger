<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module>

    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.user.theme.standard.Standard"/>
    <inherits name="ca.nanometrics.gflot.GFlot"/>
    <inherits name="com.google.gwt.resources.Resources" />

    <inherits name="com.google.gwt.activity.Activity" />
    <inherits name="com.google.gwt.place.Place" />
    <inherits name="com.sencha.gxt.cell.core.Core"/>
    <inherits name="com.sencha.gxt.ui.GXT"/>
    <inherits name="com.google.gwt.i18n.I18N"/>

    <inherits name="com.griddynamics.jagger.util" />

    <set-property name="gflot.load.flot.text" value="ENABLED" />
    <set-property name="gflot.load.canvas2image" value="ENABLED" />

    <entry-point class='com.griddynamics.jagger.webclient.client.JaggerWebClient'/>

    <stylesheet src="/hosting.css"/>
</module>
